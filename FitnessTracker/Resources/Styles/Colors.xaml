    <Color x:Key="FitnessTrackerSeries3Light">#F4890B</Color>
    <Color x:Key="FitnessTrackerSeries1Light">#2196F3</Color>
    <Color x:Key="FitnessTrackerSeries7Light">#00E190</Color>
    <Color x:Key="FitnessTrackerSeries4Light">#E2227E</Color>
    <Color x:Key="FitnessTrackerSecondaryBackgroundLight">#E8DEF8</Color>
    <Color x:Key="FitnessTrackerBorderLight">#CAC4D0</Color>
    <Color x:Key="FitnessTrackerBorderAlt1Light">#79747E</Color>
    <Color x:Key="FitnessTrackerPlaceholderForegroundLight">#49454F</Color>
    <Color x:Key="Secondary-text-colorLight">#3C3B3D</Color>
