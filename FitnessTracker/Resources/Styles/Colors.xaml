    <Color x:Key="FitnessTrackerContentBackgroundAlt1Light">#F7F2FB</Color>
    <Color x:Key="FitnessTrackerContentBackgroundAlt2Light">#F3EDF7</Color>
    <Color x:Key="FitnessTrackerSeries5Light">#116DF9</Color>
